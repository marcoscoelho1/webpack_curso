{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.Iterable",
            "esnext"
        ],
        "esModuleInterop": true,
        "module": "esnext",
        "jsx": "react",
        "rootDir": "src",
        "baseUrl": "src",
        "allowJs": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "noEmit": true,
    },
    "include": [
        "src"
    ]
}